//Write a C program to enter length and breadth of a rectangle and find its area
#include <stdio.h>

int main()
{
    float length, width, area;
    printf("Enter length of rectangle: ");
    scanf("%f", &length);
    printf("Enter width of rectangle: ");
    scanf("%d", &width);
    printf("Area of rectangle = %f", area);

    return 0;
}

//C program to find area of trapezium.
#include <stdio.h>
 
int main()
{
    float a, b, h;
    float area;
 
    printf("Enter the value for two bases & height of the trapezium:");
    scanf("%f%f%f", &a, &b, &h);
    area = 0.5 * (a + b) * h ;
    printf("Area of the trapezium is: %.3f", area);
    return 0;

//Write a C program to find power of any number x ^ y.
#include<studio.h>
#include<math.h>

int main()
{
float x,y,z;
scanf("%f%f",&x,&y);
z=pow(x,y);
printf("%f",z);
return 0;
}

//C Program to Swap Two Numbers.
#include<stdio.h>  
 int main()    
{    
int a=10, b=20;      
printf("Before swap a=%d b=%d",a,b);      
a=a+b;    
b=a-b;   
a=a-b;  
printf("\nAfter swap a=%d b=%d",a,b);    
return 0;  
}  

//Write a C program to enter P, T, R and calculate Simple Interest.
#include <stdio.h>
int main()
{
    float principle, time, rate, SI;

    printf("Enter principle (amount):");
    scanf("%f", &principle);

    printf("Enter time:");
    scanf("%f", &time);

    printf("Enter rate:");
    scanf("%f", &rate);

   
    SI=(principle*time*rate)/100;

    
    printf("Simple Interest=%f",SI);

    return 0;
}

//C Program to find area of triangle using heronâ€™s formula.
#include<stdio.h>
int main()
{

     float a,b,c,s,area;

      

     printf("Enter the length of sides of triangle");

     scanf("%f %f %f",&a,&b,&c);

      

     s = (a+b+c)/2; 

     area = (sqrt)(s*(s-a)*(s-b)*(s-c));

      

     printf("Area of triangle =%f",area);

     getch();

}

//Write a C program to enter any number and calculate its square root
#include <stdio.h>
int main()
{
    double num, root;
    printf("Enter any number to find square root:");
    scanf("%f", &num);
    root = sqrt(num);
    printf("Square root of %lf = %f", num, root);

    return 0;
}

