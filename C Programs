//Write a C program to calculate area of an equilateral triangle.
#include <stdio.h>
int main()
{
    float side, area;
    printf("Enter side of an equilateral triangle:");
    scanf("%f", &side);
    area = (sqrt(3)/4)*(side*side);
    printf("Area of equilateral triangle = %.2f sq. units", area);

    return 0;
}

//C program to find volume of sphere.
#include<stdio.h>
int main()
{
   int volume;
   int radius;
   radius=20;
   volume=((4.0f/3.0f) * (3.1415) * radius* radius* radius);
   printf("the volume of a sphere is %f",volume);
   return 0;
}

